(require '[joker.http :as http]
         '[joker.json :as json])

(def request-map 
  {:url "http://character.totalpartykill.ca/basic/json/"})

(defn parse-json [jsonData]
  (joker.json/read-string jsonData {:keywords? true}))

(defn generate-character []
  (parse-json 
    (:body (joker.http/send request-map))))
    
